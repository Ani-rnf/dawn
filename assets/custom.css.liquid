  margin: 0px;
}
h4 {  font-size: 16px;}
h5{
  font-size:20px;
}
.home_page_banner .content-container:after{
  display:none;
}
.home_page_banner .content-container:before {
    position: absolute;
    content: '';
    width: 100%;
    height: 100%;
    top: 0px;
    left: 0px;
    z-index: -1;
    /*background-color: rgba(255,255,255,0.7);*/
    background-color: #07ff02;
    opacity: 0.1;
}

.home_page_banner .banner__text.subtitle {
    color: #414342;
  font-size:16px;
    line-height: 1.3;
}
.home_page_banner .banner__heading {
    margin-bottom: 0;
    color: #414342;
    font-weight: 400;
    font-size:64px;
    line-height: 1;
    letter-spacing: 1.2px;
}
.secondary_image_with_text_clm h2{
  line-height:1 !important;
}
.custom_btn_blue {
  font-size:16px;
  color:#fff;
  text-transform:uppercase;
  display:inline-block;
  background-color:#1a9bf9;
  height: initial !important;
  line-height: 1 !important;
  opacity:1;
  padding: 14px 33px !important;
  border: none;
  text-decoration: none;
  transition: 0.5s linear;
  -webkit-transition: 0.5s linear;
}
a.button.custom_btn_blue.button--primary {
    opacity: 1 !important;
}
.custom_btn_blue:hover {
    background-color: #265;
    border: none !important;
}
