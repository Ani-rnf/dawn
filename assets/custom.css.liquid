.form_inner_clm {
    max-width: 700px;
    margin: auto;
}

.form_container{
  display:flex;
  flex-wrap:wrap;
  justify-content: space-between;
  margin-top:25px;
}
.form_field {
    width: 31%;
}
.form_container input {
    width: 100%;
    height: 38px;
    padding: 0px 15px;
    font-size: 16px;
    color:#000;
    text-transform:uppercase;
    font-family: 'Kanit', sans-serif !important;
    font-weight:400;
    line-height:1;
    letter-spacing:1.2px;
    background-color: #fff;
    border: 1px solid #fff;
    opacity: 1;
    transition: 0.5s linear;  
}
.form_container input:focus{
  border:1px solid  #000;
}
.form_container input:focus-visible {
    outline: none;
    box-shadow: none;
}
.form_container input::-webkit-input-placeholder{
  color:#000;
}
.form_container input.join_btn{
  background-color:#1a9bf9;  
  font-weight:300;
  color:#fff;
   border:none;
  text-transform:uppercase;
  cursor: pointer;
    transition: 0.5s linear;
}
