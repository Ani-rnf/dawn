/***** custome css *****/
.page-width--narrow.cms-pages {max-width: 100%;}
body .container {max-width: 1530px;padding: 0px 20px;margin: 0px auto;}
.cms-pages .main-page-title.page-title {display: none;}
.about-us h1 {font-weight: 500;font-size: 40px;margin-bottom: 10px;}
.about-us h2 {font-size: 40px;}
.about-us .about-text {font-size: 20px;}
.about-us .about-text p {margin: 15px 0px;text-align:left;}
.about-us .subtitle {width: 100%;display: inline-block;font-size: 20px;}
.about-us img {border: 0;box-shadow: none;}
.about-us .thumb {padding: 50px 0px 20px;}
.about-us h3 {font-size: 30px;width: 100%;display: inline-block;}
.about-us.center {position: relative;}
.banner-position {position: absolute;top: 50%;left: 50%;transform: translate(-50%, -50%);width: 100%;max-width: 1000px;color: #fff;font-size: 20px;}
.banner-position img {width: 200px;}
.banner-position span {font-size: 23px;font-weight: 600;width: 100%;display: inline-block;padding: 20px 0px;}
.banner-position p {font-size: 20px;margin: 20px 0px 0px;}
.cms-pages .custom_btn_blue {color: #fff !important;}
.founders-info {background: #49c29B;margin: 80px 0px;padding: 50px 0px;}
.founders-info h2 {color: #fff;font-size: 40px;text-align: center;display: inline-block;width: 100%;margin:0px 0px 30px;}
.profile-info {display: flex;color: #fff;}
.info-left {width: 50%;padding-right: 30px;}
.info-right {width: 50%;padding-left: 30px;}
.founders-info h3 {color: #fff;font-size: 30px;}
.founders-info em {font-size: 20px;font-weight: 600;}
.founders-info p {font-size: 18px;}
.template-page-our-team .card__information {display: none;}

.footer .footer-block__details-content .list-menu__item--link {position: relative;padding: 3px 0px 3px 15px !important;}
.footer-block__details-content .list-menu__item--link:before {content: "";background: #fff;width: 5px;height: 5px;border-radius: 100%;position: absolute;left: 0;top: 50%;transform: translateY(-50%);}
.footer .footer-block__details-content .list-menu__item--link, .copyright__content a {color: #fff;font-size: 16px;}
.footer .footer-block--newsletter {flex-flow: column;}
.footer .footer-block--newsletter>* {flex: auto;width: 100%;}
.footer .footer-block--newsletter .list-social {justify-content: flex-start;padding: 0;}
.footer .footer-block--newsletter .list-social li {padding: 7px;}
.newsletter-form__field-wrapper .field__input { background: #fff;}
.newsletter-form__field-wrapper .field__label {color: #333;font-size: 14px;font-weight: 600;line-height: 26px;}
.newsletter-form__button .icon {color: #333;}
.list-social__item a {background: #1a9bf9;color: #fff;border-radius: 9px;}
.list-social__item .icon {color: #fff;}
.footer__copyright.caption small a, .footer__copyright.caption small {font-size: 14px;}
.footer .footer__content-bottom {border-top: 0;padding-top: 0;}
.footer .email-us a svg {margin-right: 15px;}
.footer .email-us a {display: flex;align-items: center;margin: 30px 0px;font-size: 16px;color: #fff;flex-wrap: wrap;word-break: break-all;}
.footer-top h2 {font-size: 50px;margin: 0px 0px 30px;}
.footer-top .subtitle {font-size: 20px;color: #fff;}
.footer-top .footer-thumb li img {max-width: 100%;}
.footer-top .footer-thumb li {width: 25%;padding: 40px 15px;}
.footer-top .footer-thumb {margin: 0;padding: 0;display: flex;list-style-type: none;}

.header-wrapper .header {padding-top: 10px;padding-bottom: 10px;display: flex;justify-content: space-between;}
.header-wrapper .header > .header__heading-link {padding: 0;}
.header-wrapper .header__icons {display: none;}
.announcement-bar .announcement-bar__message {color: #fff;font-size: 15px;text-transform: uppercase;font-weight: 500;}
.header .header__menu-item:hover span {text-decoration: none;}
nav .list-menu__item:hover {color: #1a9bf9;}
nav .list-menu__item {text-transform: uppercase;}
.template-page-the-power-of-your-tree .full-unstyled-link {font-size: 25px;}
#ContactForm .field__input {background: #fff;}

@media (max-width: 991px) {
.footer-top h2 {font-size: 25px;margin: 0 0 15px;}
.footer-top .subtitle {font-size: 14px;}
.footer-top .footer-thumb {flex-wrap: wrap;}
.footer-top .footer-thumb li {width: 33.333%;padding: 10px;}
.header .menu-drawer__navigation {padding: 10px 0px;}
.header .menu-drawer .list-social {margin-left: 0;gap: 10px;}
}

@media (max-width: 767px) {
    .banner__content .banner__heading.h1 {font-size: 26px;}
  .about-us h1 {font-size: 30px;}
  .about-us .about-text {font-size: 16px;}
  .founders-info {margin: 40px 0px;padding: 30px 0px;}
  .founders-info h2 {margin:0px 0px 20px;}
  .profile-info {flex-wrap: wrap;}
  .info-left {width: 100%;padding-right: 0px;padding-bottom: 30px;}
  .info-right {width: 100%;padding-left: 0px;}
  .founders-info em {font-size: 17px;}
  .founders-info p {font-size: 16px;}
  body .image-with-text__content {padding: 3rem 0px;}
}

@media (max-width: 575px) {
.footer-top h2 {font-size: 20px;}
.footer-top .subtitle {font-size: 12px;}
.footer .footer-block__details-content {margin-bottom: 0rem;}
.footer .footer-block.grid__item {margin: 1rem 0;}
.footer .footer-block__heading {font-size: 20px!important;}
.footer .email-us a {margin: 15px 0;}
.newsletter-form__field-wrapper {max-width: 100%;}
}